{
  "manifest_version": 3,
  "name": "Auto Refresh",
  "version": "1.0",
  "permissions": ["tabs"],
  "author": "Muhammad Owais",
  "background": {
    "service_worker": "background.js"
  },
  "action": { "default_popup": "index.html", "default_title": "Refresh Page" },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
